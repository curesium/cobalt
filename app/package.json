{
  "name": "tantalum",
  "version": "1.0.0",
  "description": "Tool to calculate number of passwords possible with variable limitations",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"TANTALUM\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "\u001b[2~\u001b[2~\u001b[2~\u001b[2~\u001b[2~https://github.com/curesium/tantalum"
  },
  "keywords": [
    "calculator"
  ],
  "author": "CURESIUM",
  "license": "GPL-3.0",
  "dependencies": {
    "electron": "^11.0.2"
  },
  "devDependencies": {
    "electron-package": "^0.1.0",
    "webpack": "^5.6.0"
  }
}
